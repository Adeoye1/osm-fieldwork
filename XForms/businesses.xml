<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>businesses_mapping_v2</h:title>
    <model odk:xforms-version="1.0.0">
      <instance>
        <data id="businesses_mapping_v2">
          <start/>
          <end/>
          <today/>
          <username/>
          <phonenumber/>
          <deviceid/>
          <title/>
          <osm_business/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/data/today" type="date"/>
      <bind jr:preload="property" jr:preloadParams="username" nodeset="/data/username" type="string"/>
      <bind jr:preload="property" jr:preloadParams="phonenumber" nodeset="/data/phonenumber" type="string"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/data/deviceid" type="string"/>
      <bind nodeset="/data/title" readonly="true()" type="string"/>
      <bind nodeset="/data/osm_business" required="true()" type="binary"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/title">
      <label>MM Business Facilities Form</label>
      <hint>This is a MM building facilities survey used to collect information on businesses</hint>
    </input>
    <upload mediatype="osm/*" ref="/data/osm_business">
      <label>MM Business Facilities Form</label>
      <hint>Please select a building and tag it appropriately.</hint>
      <tag key="addr:city">
        <label>Name of city</label>
        <item>
          <label>Monrovia</label>
          <value>monrovia</value>
        </item>
      </tag>
      <tag key="addr:community">
        <label>Name of community</label>
        <item>
          <label>Duala</label>
          <value>duala</value>
        </item>
        <item>
          <label>New Kru Town</label>
          <value>new kru town</value>
        </item>
        <item>
          <label>Tweh Farm</label>
          <value>tweh farm</value>
        </item>
      </tag>
      <tag key="addr:street">
        <label>Name of street</label>
      </tag>
      <tag key="name">
        <label>Name of business (if present)</label>
      </tag>
      <tag key="amenity">
        <label>Type of amenity</label>
        <item>
          <label>ATM</label>
          <value>atm</value>
        </item>
        <item>
          <label>Bar</label>
          <value>bar</value>
        </item>
        <item>
          <label>Bank</label>
          <value>bank</value>
        </item>
        <item>
          <label>Foreign Exchange</label>
          <value>bureau_de_change</value>
        </item>
        <item>
          <label>Cafe</label>
          <value>cafe</value>
        </item>
        <item>
          <label>Car wash</label>
          <value>car_wash</value>
        </item>
        <item>
          <label>Cinema</label>
          <value>cinema</value>
        </item>
        <item>
          <label>Community centre</label>
          <value>community_centre</value>
        </item>
        <item>
          <label>Electronics</label>
          <value>electronics</value>
        </item>
        <item>
          <label>Fuel / Gas Station</label>
          <value>fuel</value>
        </item>
        <item>
          <label>Library</label>
          <value>library</value>
        </item>
        <item>
          <label>Pub</label>
          <value>pub</value>
        </item>
        <item>
          <label>Pharmacy</label>
          <value>pharmacy</value>
        </item>
        <item>
          <label>Parking</label>
          <value>parking</value>
        </item>
        <item>
          <label>Police</label>
          <value>police</value>
        </item>
        <item>
          <label>Post office</label>
          <value>post_office</value>
        </item>
        <item>
          <label>Restaurant</label>
          <value>restaurant</value>
        </item>
        <item>
          <label>Social centre</label>
          <value>social_centre</value>
        </item>
        <item>
          <label>Theatre</label>
          <value>theatre</value>
        </item>
        <item>
          <label>Townhall</label>
          <value>townhall</value>
        </item>
        <item>
          <label>Studio</label>
          <value>studio</value>
        </item>
        <item>
          <label>Hospital</label>
          <value>hospital</value>
        </item>
        <item>
          <label>Clinic</label>
          <value>clinic</value>
        </item>
        <item>
          <label>Doctors</label>
          <value>doctors</value>
        </item>
        <item>
          <label>None</label>
          <value>none</value>
        </item>
      </tag>
      <tag key="shop">
        <label>Type of shop</label>
        <item>
          <label>Hairdresser</label>
          <value>hairdresser</value>
        </item>
        <item>
          <label>Bakery</label>
          <value>bakery</value>
        </item>
        <item>
          <label>Books</label>
          <value>books</value>
        </item>
        <item>
          <label>Butcher</label>
          <value>butcher</value>
        </item>
        <item>
          <label>Convenience</label>
          <value>convenience</value>
        </item>
        <item>
          <label>Fruits and/or vegetables</label>
          <value>greengrocer</value>
        </item>
        <item>
          <label>Provisions / general</label>
          <value>general</value>
        </item>
        <item>
          <label>Boutique</label>
          <value>boutique</value>
        </item>
        <item>
          <label>Clothes</label>
          <value>clothes</value>
        </item>
        <item>
          <label>Fashion</label>
          <value>fashion</value>
        </item>
        <item>
          <label>Shoes</label>
          <value>shoes</value>
        </item>
        <item>
          <label>Tailor</label>
          <value>tailor</value>
        </item>
        <item>
          <label>Beauty</label>
          <value>beauty</value>
        </item>
        <item>
          <label>Cosmetics</label>
          <value>cosmetics</value>
        </item>
        <item>
          <label>Craft</label>
          <value>craft</value>
        </item>
        <item>
          <label>Furniture</label>
          <value>furniture</value>
        </item>
        <item>
          <label>Seafood</label>
          <value>seafood</value>
        </item>
        <item>
          <label>saloon</label>
          <value>saloon</value>
        </item>
        <item>
          <label>Cold Storage / Frozen food</label>
          <value>frozen_food</value>
        </item>
        <item>
          <label>Supermarket</label>
          <value>supermarket</value>
        </item>
        <item>
          <label>Garage / Car repair</label>
          <value>car_repair</value>
        </item>
        <item>
          <label>None</label>
          <value>none</value>
        </item>
      </tag>
      <tag key="operator">
        <label>Name of operator (company running the amenity or facility)</label>
      </tag>
      <tag key="operator:type">
        <label>What type of operator is this?</label>
        <item>
          <label>Government</label>
          <value>government</value>
        </item>
        <item>
          <label>NGO</label>
          <value>ngo</value>
        </item>
        <item>
          <label>Community</label>
          <value>community</value>
        </item>
        <item>
          <label>Religious</label>
          <value>religious</value>
        </item>
        <item>
          <label>Public</label>
          <value>public</value>
        </item>
        <item>
          <label>Private individual</label>
          <value>private_individual</value>
        </item>
        <item>
          <label>Union/Association</label>
          <value>union_association</value>
        </item>
      </tag>
      <tag key="opening_hours">
        <label>Days/times of Opening the business</label>
        <item>
          <label>24/7</label>
          <value>24/7</value>
        </item>
        <item>
          <label>Mo-Sa 08:00-18:00</label>
          <value>Mo-Sa 08:00-18:00</value>
        </item>
        <item>
          <label>Mo-Sa 09:00-17:00</label>
          <value>Mo-Sa 09:00-17:00</value>
        </item>
        <item>
          <label>Mo-Sa 09:00-18:00</label>
          <value>Mo-Sa 09:00-18:00</value>
        </item>
        <item>
          <label>Mo-Fr 09:00-17:00</label>
          <value>Mo-Fr 09:00-17:00</value>
        </item>
        <item>
          <label>Mo-Su 09:00-21:00</label>
          <value>Mo-Su 09:00-21:00</value>
        </item>
        <item>
          <label>Mo-Fr 08:00-18:00; Sa-Su 08:00-15:00</label>
          <value>Mo-Fr 08:00-18:00; Sa-Su 08:00-15:00</value>
        </item>
        <item>
          <label>By Appointment Only</label>
          <value>by_appointment</value>
        </item>
      </tag>
      <tag key="comment">
        <label>Any other comments about this business</label>
      </tag>
    </upload>
  </h:body>
</h:html>
